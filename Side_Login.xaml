<Page x:Class="WpfApp1_SEFP_program_test2.Side_Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1_SEFP_program_test2"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Side_Login">

    <Grid>
        <Label Content="Silkeborg Easy Football Planner" HorizontalAlignment="Center" Margin="0,45,0,0" VerticalAlignment="Top"/>
        <Label Content="Medlem-ID" HorizontalAlignment="Left" Margin="322,114,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="UsernameBox" HorizontalAlignment="Center" Margin="0,145,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="138"/>
        <Label Content="Adgangskode" HorizontalAlignment="Left" Margin="322,178,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="PasswordBox" HorizontalAlignment="Center" Margin="0,209,0,0" VerticalAlignment="Top" Width="138"/>
        <Button Content="Login" HorizontalAlignment="Center" Margin="0,252,0,0" VerticalAlignment="Top" Width="138" Click="Side_Login_Log_ind_Klik"/>
        <Button Content="Opret Medlem" HorizontalAlignment="Center" Margin="0,325,0,0" VerticalAlignment="Top" Width="138" Click="Opret_Medlem_Klik"/>
    </Grid>
</Page>
