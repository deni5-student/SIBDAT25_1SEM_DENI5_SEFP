<Page x:Class="WpfApp1_SEFP_program_test2.Side_Medlem_Dashboard"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1_SEFP_program_test2"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Side_Medlem_Dashboard">

    <Grid>
        <Label Content="Medlem dash board" HorizontalAlignment="Center" Margin="0,76,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="Aktivitet_List" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="218,112,218,112"/>
        <Button Content="Tilmeld dig aktivitet" HorizontalAlignment="Center" Margin="0,362,0,0" VerticalAlignment="Top" Width="138" Click="Tilmeld_Aktivitet_knap_klik"/>
        <Button Content="Log ud" HorizontalAlignment="Left" Margin="515,362,0,0" VerticalAlignment="Top" Width="138" Click="Medlem_logout_knap_klik"/>
        <Label Content="Silkeborg Easy Football Planner" HorizontalAlignment="Center" Margin="0,45,0,0" VerticalAlignment="Top"/>

    </Grid>
</Page>
